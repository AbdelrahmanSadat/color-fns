{
  "name": "color-fns",
  "version": "0.0.6",
  "description": "Modern JavaScript color utility library.",
  "author": "",
  "main": "dist/color-fns.js",
  "module": "dist/color-fns.esm.js",
  "unpkg": "dist/color-fns.js",
  "license": "MIT",
  "keywords": [
    "color",
    "utility",
    "functions"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint ./src --fix",
    "build:esm": "node build/esm",
    "build:umd": "node build/umd",
    "build": "npm run lint && npm run build:esm && npm run build:umd"
  },
  "maintainers": [
    {
      "name": "Abdelrahman3D",
      "email": "abdelrahman3d@gmail.com"
    }
  ],
  "eslintIgnore": [
    "test",
    "dist"
  ],
  "jest": {
    "testMatch": [
      "**/test/**/*.js"
    ]
  },
  "files": [
    "dist/*.js"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "chalk": "^2.3.2",
    "eslint": "^4.19.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "filesize": "^3.6.0",
    "gzip-size": "^4.1.0",
    "jest": "^22.4.2",
    "rollup": "^0.56.5",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-browsersync": "^0.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "uglify-js": "^3.3.15"
  }
}
